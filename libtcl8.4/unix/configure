EOF

	    echo "$ac_t""FIONBIO" 1>&6
	    ;;
	SunOS-4*)
	    cat >> confdefs.h <<\EOF
#define USE_FIONBIO 1
EOF

	    echo "$ac_t""FIONBIO" 1>&6
	    ;;
	ULTRIX-4.*)
	    cat >> confdefs.h <<\EOF
#define USE_FIONBIO 1
EOF

	    echo "$ac_t""FIONBIO" 1>&6
	    ;;
	*)
	    echo "$ac_t""O_NONBLOCK" 1>&6
	    ;;
    esac


