<!-- body begins here -->
<refsynopsisdiv xml:id="synopsis">
  <cmdsynopsis sepchar=" ">
    <command>tor-rcc</command>    
    
    <arg choice="req" rep="norepeat"><replaceable>torname</replaceable></arg>
