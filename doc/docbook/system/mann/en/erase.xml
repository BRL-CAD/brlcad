<refnamediv xml:id="name">
  <refname>erase</refname>
  <refpurpose>Deletes the specified list of objects from the MGED display.
   </refpurpose>
</refnamediv>

<!-- body begins here -->
<refsynopsisdiv xml:id="synopsis">
  <cmdsynopsis sepchar=" ">
    <command>erase</command>    
    
    <arg choice="req" rep="norepeat"><replaceable>erase</replaceable></arg>
    <arg choice="opt" rep="norepeat"/>
  </cmdsynopsis>
