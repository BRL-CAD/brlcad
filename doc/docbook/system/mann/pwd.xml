<refentry xmlns="http://docbook.org/ns/docbook" version="5.0" xml:id="pwd">

  <refmeta>
    <refentrytitle>PWD</refentrytitle>
    <manvolnum>nged</manvolnum>
    <refmiscinfo class="source">BRL-CAD</refmiscinfo>
    <refmiscinfo class="manual">BRL-CAD User Commands</refmiscinfo>
  </refmeta>

  <refnamediv xml:id="name">
    <refname>pwd</refname>
    <refpurpose>Prints the current working directory.
    </refpurpose>
  </refnamediv>

  <!-- body begins here -->
  <refsynopsisdiv xml:id="synopsis">
    <cmdsynopsis sepchar=" ">
      <command>pwd</command>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsection xml:id="description"><title>DESCRIPTION</title>

    <para>
      Prints the current working directoy. See also the <command>cd</command> command.
    </para>
  </refsection>

  <refsection xml:id="examples"><title>EXAMPLES</title>

    <para>
      Print the current working directory.
    </para>
    <example><title>Type <command>pwd</command> to display the current working directory.
      </title>

      <variablelist>
	<varlistentry>
	  <term><prompt>mged&gt; </prompt><userinput>pwd</userinput></term>
	  <listitem>
	    <para>Returns the current working directory.
	    </para>
	  </listitem>
	</varlistentry>
      </variablelist>
    </example>
  </refsection>

  <refsection xml:id="author"><title>AUTHOR</title>
