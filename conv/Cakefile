#define SRCDIR	conv
#define PRODUCTS	asc2g g2asc conv-vg2g pix2asc asc2pix euclid-g \
			g-euclid euclid_format euclid_unformat tankill-g g-tankill \
			viewpoint-g dxf-g g-nmg g-euclid1 fast4-g proe-g \
			asc-nmg nmg-rib g-vrml g-acad cy-g nastran-g g-wave \
			nmg-polysolid g-shell.rect stl-g
#define	SRCSUFF	.c
#define MANSECTION	1

#define STATIC_PRODUCTS ../SRCDIR/pro_engineer

#include "../Cakefile.defs"
#include "../Cakefile.prog"

/* Explicit composition of each product */

nmg-polysolid: nmg-polysolid.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nmg-polysolid.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o nmg-polysolid

g-sgp: g-sgp.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-sgp.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-sgp

nmg-sgp: nmg-sgp.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nmg-sgp.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -lgen -o nmg-sgp

nastran-g: nastran-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS nastran-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o nastran-g

g-acad: g-acad.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-acad.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-acad

g-wave: g-wave.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-wave.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-wave

g-shell.rect: g-shell.rect.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-shell.rect.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-shell.rect

proe-g: proe-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP LIBSYSV_DEP
	CC LDFLAGS proe-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES LIBSYSV -o proe-g

stl-g:	proe-g
	rm -f stl-g
	ln -s proe-g stl-g

fast4-g: fast4-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS fast4-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o fast4-g

asc2g:	asc2g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS asc2g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o asc2g

g2asc:	g2asc.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP
	CC LDFLAGS g2asc.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g2asc

conv-vg2g:	conv-vg2g.o
	CC LDFLAGS conv-vg2g.o -o conv-vg2g

pix2asc:	pix2asc.o
	CC LDFLAGS pix2asc.o -o pix2asc

asc2pix:	asc2pix.o
	CC LDFLAGS asc2pix.o -o asc2pix

cy-g:	cy-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS cy-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o cy-g

euclid-g:	euclid-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS euclid-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o euclid-g

g-euclid:	g-euclid.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-euclid.o LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-euclid

g-euclid1:	g-euclid1.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-euclid1.o LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-euclid1

euclid_format:	euclid_format.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP
	CC LDFLAGS euclid_format.o LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o euclid_format

euclid_unformat:	euclid_unformat.o
	CC LDFLAGS euclid_unformat.o -o euclid_unformat

tankill-g:	tankill-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS tankill-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o tankill-g

g-tankill:	g-tankill.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP
	CC LDFLAGS g-tankill.o LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-tankill

viewpoint-g:	viewpoint-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS viewpoint-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o viewpoint-g

dxf-g:	dxf-g.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS dxf-g.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o dxf-g

g-nmg:	g-nmg.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS g-nmg.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-nmg

asc-nmg: asc-nmg.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP LIBWDB_DEP
	CC LDFLAGS asc-nmg.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o asc-nmg

nmg-rib: nmg-rib.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP
	CC LDFLAGS nmg-rib.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o nmg-rib

g-vrml: g-vrml.o LIBRT_DEP LIBBN_DEP LIBBU_DEP LIBTCL_DEP	
	CC LDFLAGS g-vrml.o LIBWDB LIBRT LIBBN LIBBU LIBBU_LIBES LIBTCL LIBTCL_LIBES LIBES -o g-vrml

#include "../Cakefile.rules"
