<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>

	<mime-type type="application/x-brlcad-unknown">
		<comment>BRL-CAD Unknown</comment>
		<glob pattern="*.g"/>
	</mime-type>

	<mime-type type="application/x-brlcad-v4">
		<comment>BRL-CAD Geometry Database v4</comment>
		<icon name="application-x-brlcad-v4"/>
		<magic priority="50">
			<match value="\x49" type="string" offset="0">
				<match value="\x76\x34" type="string" offset="2"/>
			</match>
		</magic>
		<glob pattern="*.g"/>
	</mime-type>

	<mime-type type="application/x-brlcad-v5">
		<comment>BRL-CAD Geometry Database v5</comment>
		<icon name="application-x-brlcad-v5"/>
		<magic priority="50">
			<match value="\x76" type="string" offset="0">
				<match value="\x35\x76" type="string" offset="7"/>
			</match>
		</magic>
		<glob pattern="*.g"/>
	</mime-type>

</mime-info>
