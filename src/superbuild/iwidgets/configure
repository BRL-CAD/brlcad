	    echo "$as_me:$LINENO: result: found $itcl_BIN_DIR/itclConfig.sh" >&5
echo "${ECHO_T}found $itcl_BIN_DIR/itclConfig.sh" >&6
	fi
    fi


    echo "$as_me:$LINENO: checking for existence of ${itcl_BIN_DIR}/itclConfig.sh" >&5
