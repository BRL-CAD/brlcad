	    itcl_BIN_DIR="# no itcl configs found"
	    { echo "$as_me:$LINENO: WARNING: \"Cannot find itcl configuration definitions\"" >&5
echo "$as_me: WARNING: \"Cannot find itcl configuration definitions\"" >&2;}
	    exit 0
	else
