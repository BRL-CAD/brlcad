               'ASSOCIATIVE_DRAUGHTING.LEADER_CURVE' IN TYPEOF(l_1)) )) >= 
               1;
      wr2: SIZEOF(SELF\draughting_callout.contents) >= 2;
  END_ENTITY; -- leader_directed_callout

  ENTITY leader_directed_dimension
    SUBTYPE OF (leader_directed_callout);
    WHERE
