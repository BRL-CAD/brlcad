      wr2: (SIZEOF(QUERY ( item <* SELF\representation.items | (NOT (
               SIZEOF(['TECHNICAL_DATA_PACKAGING.TEXT_LITERAL',
               'TECHNICAL_DATA_PACKAGING.ANNOTATION_TEXT',
