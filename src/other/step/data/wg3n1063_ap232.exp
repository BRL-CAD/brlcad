               item)) ) | (NOT (SIZEOF(QUERY ( elem <* gcs\geometric_set.
               elements | (NOT (SIZEOF(TYPEOF(elem) * [
               'TECHNICAL_DATA_PACKAGING.B_SPLINE_CURVE',
