FUNCTION convert_to_operands(values :  AGGREGATE OF maths_value) : LIST OF 
  generic_expression;
LOCAL
  operands : LIST OF generic_expression := [];
  loc : INTEGER := 0;
END_LOCAL;
  IF NOT EXISTS(values) THEN
    RETURN (?);
