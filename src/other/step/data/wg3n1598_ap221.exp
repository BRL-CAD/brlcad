WHERE
  wr1 : (space_dimension(fun.domain) = 1) AND (space_dimension(fun.range) = 1);
  wr2 : number_tuple_subspace_check(factor1(fun.range));
  wr3 : space_dimension(factor1(fun.range)) > 1;
