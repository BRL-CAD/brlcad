FUNCTION valid_selected_instance_representation (pd: product_definition_or_assembly_relationship): LOGICAL;
LOCAL
  properties: SET OF property_definition := bag_to_set(QUERY( prd<* USEDIN ( pd ,'AP236_FURNITURE_CATALOG_AND_INTERIOR_DESIGN_MIM_LF.PROPERTY_DEFINITION.DEFINITION' ) | 
