   WR1: u1 <> u2;
   WR2: v1 <> v2;
   WR3: (('AP236_FURNITURE_CATALOG_AND_INTERIOR_DESIGN_MIM_LF.ELEMENTARY_SURFACE' IN TYPEOF(basis_surface))
