   WR4: (('AP236_FURNITURE_CATALOG_AND_INTERIOR_DESIGN_MIM_LF.SPHERICAL_SURFACE' IN TYPEOF(basis_surface))
          OR
         ('AP236_FURNITURE_CATALOG_AND_INTERIOR_DESIGN_MIM_LF.TOROIDAL_SURFACE' IN TYPEOF(basis_surface)))
