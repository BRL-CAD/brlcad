    END_IF;
    IF 'INTEGRATED_CNC_SCHEMA.PATH' IN TYPEOF(an_item) THEN
      RETURN (path_reversed(an_item));
    END_IF;
    IF 'INTEGRATED_CNC_SCHEMA.FACE_BOUND' IN TYPEOF(an_item) THEN
