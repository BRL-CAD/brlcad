     REPEAT i:= 1 TO SIZEOF(sdqir);
       sdir := sdir + QUERY(q <* bag_to_set(USEDIN(sdqir[i],
                   'AP209_MULTIDISCIPLINARY_ANALYSIS_AND_DESIGN_MIM_LF.'+
                   'DATA_QUALITY_INSPECTION_REPORT.'+
