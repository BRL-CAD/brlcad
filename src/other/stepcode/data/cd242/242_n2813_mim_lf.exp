  WR2:  SIZEOF( QUERY( sa <* get_shape_aspects(SELF) |
              (sa.name = 'barring profile') AND
              (sa.description = 'closed profile occurrence') AND
              (SIZEOF( QUERY( sar <* USEDIN( sa, 'AP242_MANAGED_MODEL_BASED_3D_ENGINEERING_MIM_LF.' +
