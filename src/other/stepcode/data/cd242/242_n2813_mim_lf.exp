             (SIZEOF( QUERY( pdr <* get_shape_aspect_property_definition_representations(SELF) |
               ('AP242_MANAGED_MODEL_BASED_3D_ENGINEERING_MIM_LF.SHAPE_REPRESENTATION_WITH_PARAMETERS' IN 
