               ('AP242_MANAGED_MODEL_BASED_3D_ENGINEERING_MIM_LF.JOGGLE' IN TYPEOF(sar.related_shape_aspect.of_shape.definition)) AND
               (sar.related_shape_aspect.of_shape.definition\characterized_object.name = 'termination') AND
