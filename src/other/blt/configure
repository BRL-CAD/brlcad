fi
fi
rm -fr conftest*
    CFLAGS="$old_flags"
    cat >> confdefs.h <<EOF
#define BLT_DBL_EPSILON ${blt_cv_dbl_epsilon}
EOF

    echo "$ac_t""${blt_cv_dbl_epsilon}" 1>&6

fi

