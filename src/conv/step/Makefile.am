	$(fedex_srcs) \
	$(fedex_headers)

step_g_LDADD = \
	${BU} \
	${WDB} \
	${RT} \
	${STEP_LIBS}

# per-program CPPFLAGS wasn't added until automake 1.7
step_g_CXXFLAGS = \
	${OPENNURBS_CPPFLAGS} \
	${STEP_CPPFLAGS} \
	${TCL_CPPFLAGS}

$(fedex_srcs):
	echo "Building fedex sources: ${fedex_srcs} ${fedex_hdrs}"
	$(STEP_FEDEX_PLUS) $(STEP_AP203)

$(fedex_hdrs): ${fedex_srcs}

include $(top_srcdir)/misc/Makefile.defs

FAST_OBJECTS = \
	$(step_g_OBJECTS) \
	$(bin_PROGRAMS)
