	:cm=\014%r%n%.%.:dc=\020\010\030\035:\
	:dl=\020\032\030\035:dm=^P:do=^J:ed=^X^]:\
	:ei=\377\377\030\035:ho=^B:ic=\020\034\030\035:im=^P:\
	:le=^H:nd=^\:pc=\377:se=^X^]:sf=^J:so=^N:up=^Z:
