	:es:hs:\
	:ds=\E7\E[?6l\E[2K\E[?6h\E8:fs=\E[?6h\E8:\
	:i1=\E%!1\E[2;32r\E[132D\E[2g\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[8C\EH\E[J:\
