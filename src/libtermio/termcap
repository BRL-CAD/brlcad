	:am:bs:\
	:co#80:li#27:\
	:bl=^G:ce=^NU:cl=^NR:cm=\017%+^P%+^P:dc=^NV:do=^J:ho=^NQ:\
	:le=^H:nd=^Y:sf=^J:up=^Z:

